{
  "private": true,
  "name": "challengemyself",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "validate": "npm-run-all --parallel check-format lint test",
    "check-format": "prettier --ignore-path .gitignore --list-different \"**/*.+(js|json)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "serve:locally": " preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/preact": "^1.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-developit": "^1.2.0",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.1.3",
    "jest": "^25.2.7",
    "eslint-plugin-mocha": "^6.2.2",
    "jest-preset-preact": "^1.0.0",
    "lint-staged": "^10.1.2",
    "npm-run-all": "^4.1.5",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.9",
    "preact-cli-svg-loader": "^1.1.0",
    "preact-render-spy": "^1.3.0",
    "prettier": "^2.0.4"
  },
  "dependencies": {
    "firebase": "^7.13.2",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^4.0.2",
    "@lottiefiles/lottie-player": "^0.3.0-rc2",
    "lottie-player": "^1.0.0",
    "lottie-web": "^5.6.7",
    "modern-normalize": "^0.6.0",
    "preact": "^10.3.4",
    "preact-portal": "^1.1.3",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-i18next": "^11.3.4",
    "react-icons": "^3.9.0",
    "vanilla-tilt": "^1.7.0"
  },
  "jest": {
    "preset": "jest-preset-preact"
  }
}
