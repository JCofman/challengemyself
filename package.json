{
  "private": true,
  "name": "challengemyself",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "validate": "npm-run-all --parallel check-format lint test",
    "check-format": "prettier --ignore-path .gitignore --list-different \"**/*.+(js|json)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "serve:locally": " preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/preact": "^1.0.2",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-developit": "^1.1.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-mocha": "^6.2.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.1",
    "preact-cli-svg-loader": "^1.0.0",
    "preact-render-spy": "^1.3.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@lottiefiles/lottie-player": "^0.3.0-rc2",
    "firebase": "^7.6.1",
    "modern-normalize": "^0.6.0",
    "preact": "^10.1.1",
    "preact-portal": "^1.1.3",
    "preact-render-to-string": "^5.1.3",
    "preact-router": "^3.1.0",
    "react-icons": "^3.8.0",
    "vanilla-tilt": "^1.7.0"
  },
  "jest": {
    "preset": "jest-preset-preact"
  }
}
